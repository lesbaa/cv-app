{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "static": "./static",
        "underscore": "lodash",
        "~": "./src"
      },
      "extensions": [
        ".js",
        ".jsx",
        ".es",
        ".es6",
        ".svg",
        ".css",
        ".sass"
      ]
    }]
  ],
  "env": {
    "development": {
      "plugins": [
        "styled-jsx/babel"
      ]
    },
    "live": {
      "plugins": [
        "transform-react-remove-prop-types",
        "styled-jsx/babel"
      ]
    }
  }
}